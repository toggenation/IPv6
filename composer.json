{
    "name": "toggen/ipv6",
    "description": "IPv6 Utilities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Toggen\\Ipv6\\": "src/"
        }
    },
    "authors": [
        {
            "name": "James McDonald",
            "email": "james@toggen.com.au"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "composer/composer": "2.7.x-dev"
    },
    "scripts-descriptions": {
        "expand": "Expand IPv6 (fe80::1) to full form (fe80:0000:0000:0000:0000:0000:0000:0001)",
        "ip": "Convert IPv6 to binary and show bit where subnet ends",
        "mac": "Convert a MAC address (00:ff:7a:8d:b2:4e) to an IPv6 address (fe80::2ff7:afff:e8db:24e)"
    },
    "scripts": {
        "expand": [
            "Toggen\\Ipv6\\Runner::expand"
        ],
        "ip": [
            "Toggen\\Ipv6\\Runner::run"
        ],
        "mac": [
            "Toggen\\Ipv6\\Runner::mac"
        ]
    }
}